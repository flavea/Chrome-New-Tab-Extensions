{
   "background": {
      "page": "index.html"
   },
   "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "options_ui": {
      "page": "options.html",
      "chrome_style": true,
      "open_in_tab": true
   },
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "manifest_version": 2,
   "content_security_policy": "script-src 'self' https://*.twitter.com https://*.twimg.com 'unsafe-eval'; object-src 'self'",
   "name": "Paper Heart - A Custom New Tab Page",
   "short_name": "Paper Heart",
   "author": "Ilma Arifiany",
   "description": "A custom new tab page.",
   "version": "1.0.0.0",
   "permissions": ["storage", "https://*.twitter.com/", "https://*.twimg.com/", "history", "activeTab"],
   "homepage_url": "http://hellolittlered.org",
   "offline_enabled": true,
   "web_accessible_resources": ["*/*", "/*"]
}